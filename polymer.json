{
  "builds": [
    {
      "basePath": true,
      "browserCapabilities": ["es2015"],
      "js": {
        "compile": false,
        "minify": false
      },
      "name": "modern-es6",
      "preset": "es6-unbundled"
    },
    {
      "basePath": true,
      "name": "fallback-es5",
      "preset": "es5-bundled"
    }
  ],
  "entrypoint": "index.html",
  "extraDependencies": [
    "bower_components/webcomponentsjs/webcomponents-loader.js",
    "bower_components/app-storage/app-indexeddb-mirror/app-indexeddb-mirror-worker.js",
    "bower_components/app-storage/app-indexeddb-mirror/common-worker-scope.js",
    "server/app.yaml",
    "manifest.json"
  ],
  "fragments": [
    "src/view-dashboard.html",
    "src/view-setup.html",
    "src/view-plan.html",
    "src/view-track.html",
    "src/view-settings.html",
    "src/view-login.html",
    "src/view-404.html",
    "src/view-help.html",
    "src/view-term.html",
    "src/view-error.html",
    "src/view-offline.html",
    "src/lazy-resources.html"
  ],
  "lint": {
    "ignoreWarnings": ["undefined-elements", "set-unknown-attribute"],
    "rules": ["polymer-2"]
  },
  "shell": "src/view-app.html",
  "sources": [
    "src/**/*",
    "data/**/*",
    "images/**/*",
    "lib/**",
    "bower.json",
    "index.html"
  ]
}
