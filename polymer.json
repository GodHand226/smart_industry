{
  "entrypoint": "index.html",
  "shell": "src/view-app.html",
  "fragments": [
    "src/view-dashboard.html",
    "src/view-setup.html",
    "src/view-plan.html",
    "src/view-track.html",
    "src/view-settings.html",
    "src/view-login.html",
    "src/view-404.html",
    "src/view-error.html",
    "src/view-offline.html",
    "src/lazy-resources.html"
  ],
  "sources": [
    "src/**/*",
    "data/**/*",
    "images/**/*",
    "lib/*",
    "bower.json"
  ],
  "extraDependencies": [
    "app.yaml",
    "manifest.json",
    "bower_components/webcomponentsjs/*"
  ],
  "lint": {
    "rules": [
      "polymer-2-hybrid"
    ],
    "ignoreWarnings": [
      "undefined-elements",
      "set-unknown-attribute",
      "databinding-calls-must-be-functions",
      "databind-with-unknown-property"
    ]
  },
  "builds": [
    {
      "name": "default",
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "js": {
        "minify": false,
        "compile": false
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": false
      },
      "basePath": true,
      "bundle": false,
      "addServiceWorker": true,
      "addPushManifest": true,
      "insertPrefetchLinks:": true,
      "swPrecacheConfig": "sw-precache-config.js"
    }
  ]
}