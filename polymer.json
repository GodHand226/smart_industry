{
  "builds": [{
    "js": {
      "compile": false,
      "minify": false
    },

    "name": "default",
    "preset": "es6-unbundled"
  }],
  "entrypoint": "index.html",
  "extraDependencies": [
    "bower_components/app-storage/app-indexeddb-mirror/app-indexeddb-mirror-worker.js",
    "bower_components/app-storage/app-indexeddb-mirror/common-worker-scope.js",
    "bower_components/webcomponentsjs/*.js",
    "server/app.yaml",
    "manifest.json",
    "firebase-messaging-sw.js"
  ],
  "fragments": [
    "src/view-dashboard.html",
    "src/view-setup.html",
    "src/view-plan.html",
    "src/view-track.html",
    "src/view-settings.html",
    "src/view-login.html",
    "src/view-404.html",
    "src/view-help.html",
    "src/view-term.html",
    "src/view-error.html",
    "src/view-offline.html",
    "src/lazy-resources.html"
  ],
  "lint": {
    "ignoreWarnings": [
      "undefined-elements",
      "set-unknown-attribute"
    ],
    "rules": [
      "polymer-2"
    ]
  },
  "shell": "src/view-app.html",
  "sources": [
    "src/**/*",
    "data/**/*",
    "images/**/*",
    "lib/**",
    "bower.json"
  ]
}