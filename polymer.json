{
  "builds": [
    {
      "addPushManifest": true,
      "addServiceWorker": true,
      "basePath": true,
      "browserCapabilities": [
        "es2015",
        "push"
      ],
      "bundle": false,
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "insertPrefetchLinks:": true,
      "js": {
        "compile": false,
        "minify": false
      },
      "name": "default",
      "swPrecacheConfig": "sw-precache-config.js"
    }
  ],
  "entrypoint": "index.html",
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "app.yaml",
    "manifest.json",
    "firebase-messaging-sw.js"
  ],
  "fragments": [
    "src/view-dashboard.html",
    "src/view-setup.html",
    "src/view-plan.html",
    "src/view-track.html",
    "src/view-settings.html",
    "src/view-login.html",
    "src/view-404.html",
    "src/view-term.html",
    "src/view-error.html",
    "src/view-offline.html",
    "src/lazy-resources.html"
  ],
  "lint": {
    "ignoreWarnings": [
      "undefined-elements",
      "set-unknown-attribute"
    ],
    "rules": [
      "polymer-2"
    ]
  },
  "shell": "src/view-app.html",
  "sources": [
    "src/**/*",
    "data/**/*",
    "images/**/*",
    "lib/*"
  ]
}